<launch>

        <!-- 在gazebo中加载模型 -->
    <include file="$(find marm_gazebo)/launch/mbot_with_arm_world.launch" />
    <!-- 加载控制器插件 -->
    <rosparam file="$(find marm_gazebo)/config/arm_gazebo_joint_states.yaml" command="load"/>
    <rosparam file="$(find marm_gazebo)/config/trajectory_control.yaml" command="load"/>
    <rosparam file="$(find marm_gazebo)/config/mbot_base_control.yaml" command="load"/>
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
        ns="/arm" args="joint_state_controller arm_joint_controller gripper_controller" />

</launch>
